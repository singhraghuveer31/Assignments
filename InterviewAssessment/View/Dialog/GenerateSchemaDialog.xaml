<Window x:Class="DomainModelEditor.View.Dialog.GenerateSchemaDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Generate Schema" Height="80" Width="450"
        AutomationProperties.AutomationId="GenerateSchemaDialog">
    <StackPanel>
        <StackPanel Orientation="Horizontal"
                    VerticalAlignment="Bottom" HorizontalAlignment="Right"
                    Margin="10" Height="20">
            <Button Content="Generate" Click="Generate_Click" Width="60"
                    AutomationProperties.AutomationId="GenerateDatabase.GenerateButton" />
            <Button Content="Cancel" Click="Cancel_Click" Margin="10,0" Width="50" />
        </StackPanel>
    </StackPanel>
</Window>